df['education'] = le.fit_transform(data['education'])
df['race'] = le.fit_transform(data['race'])
df['occupation'] = le.fit_transform(data['occupation'])
df['marital.status'] = le.fit_transform(data['marital.status'])
df['relationship'] = le.fit_transform(data['relationship'])
df['sex'] = le.fit_transform(data['sex'])
df['income'] = le.fit_transform(data['income'])

df = df.replace('?', np.nan)

x = df.drop(['race', 'relationship', 'marital.status', 'income'], axis=1)
y = df['income']

x_train, x_test, y_train, y_test = train_test_split(x, y, random_state=0, test_size=0.3)

rfc.fit(x_train, y_train)
y_rfc = rfc.predict(x_test)

print("Random Forest:", accuracy_score(y_test, y_rfc))
